.container
  div.row
    div.col-md-4.middle
      p
        span.large {{ dialed*100 / total | number : 0 }}% 
        | Dialed
      p {{ dialed }} out of {{ total }} dialed
    div.col-md-4.middle
      p
        span.large {{ two_or_more*100 / dialed | number : 0 }}% 
        | {{ two_or_more }}/{{ dialed }}
      p {{ two_or_more }} stars or more
    div.col-md-4.middle
      p
        span.large {{ one_or_less*100 / dialed | number : 0 }}% 
        | {{ one_or_less }}/{{ dialed }}
      p {{ one_or_less }} stars or more
  div.row
    div.col-md-6
      p Started {{campaign.start_date | formatDate}}
      p Ending in {{timeLeft()}} days
      p {{numAgents()}} agents since start
    div.col-md-6
      h3 Call outcomes
      p(ng-repeat='(outcome, number) in outcomes')
        strong {{outcome}}: 
        | {{number}}